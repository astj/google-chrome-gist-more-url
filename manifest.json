{
    "name": "ShowMoreGistUrl",
    "version": "0.0.3",
    "manifest_version": 2,
    "description": "The Google Chrome extension that shows url to clone as 'ssh' and 'git' style on Gist.",
    "content_scripts": [
            {
              "matches": ["https://gist.github.com/*"],
              "js": ["js/jquery-1.9.1.min.js", "js/script.js"],
              "run_at": "document_start"
            }
    ]
}
